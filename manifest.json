{
    "name": "weread",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "A extension to search book in weread's bookrack, 搜索微信读书书架上的书籍",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://weread.qq.com/web/shelf"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}